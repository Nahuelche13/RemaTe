<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <object class="AdwWindow" id="_root">
    <child>
      <object class="GtkBox">
        <property name="orientation">1</property>
        <child>
          <object class="AdwHeaderBar">
            <property name="title-widget">
              <object class="AdwWindowTitle">
                <property name="title">Lotes</property>
              </object>
            </property>
          </object>
        </child>
        <child>
          <object class="GtkBox">
            <child>
              <object class="GtkBox">
                <property name="margin-bottom">24</property>
                <property name="margin-end">24</property>
                <property name="margin-start">24</property>
                <property name="margin-top">24</property>
                <property name="orientation">1</property>
                <property name="spacing">24</property>
                <child>
                  <object class="GtkLabel">
                    <property name="label">Lote</property>
                    <style>
                      <class name="title-1"/>
                    </style>
                  </object>
                </child>
                <child>
                  <object class="AdwEntryRow" id="_idEntry">
                    <style>
                      <class name="card"/>
                    </style>
                    <property name="input-purpose">7</property>
                    <property name="title">ID:</property>
                  </object>
                </child>
                <child>
                  <object class="GtkBox">
                    <property name="orientation">1</property>
                    <property name="spacing">24</property>
                    <child>
                      <object class="AdwEntryRow" id="_minEntry">
                        <style>
                          <class name="card"/>
                        </style>
                        <property name="title">Precio base:</property>
                      </object>
                    </child>
                    <child>
                      <object class="AdwEntryRow" id="_comisionEntry">
                        <style>
                          <class name="card"/>
                        </style>
                        <property name="title">Comision:</property>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkCenterBox">
                    <child type="start">
                      <object class="GtkButton" id="_deleteButton">
                        <child>
                          <object class="AdwButtonContent">
                            <property name="icon-name">user-trash-symbolic</property>
                            <property name="label">Borrar</property>
                            <property name="valign">3</property>
                          </object>
                        </child>
                        <style>
                          <class name="destructive-action"/>
                        </style>
                      </object>
                    </child>
                    <child type="end">
                      <object class="GtkButton" id="_applyButton">
                        <child>
                          <object class="AdwButtonContent">
                            <property name="icon-name">document-save-symbolic</property>
                            <property name="label">Guardar</property>
                            <property name="valign">3</property>
                          </object>
                        </child>
                        <style>
                          <class name="suggested-action"/>
                        </style>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkSeparator"></object>
            </child>
            <child>
              <object class="GtkBox">
                <property name="hexpand">true</property>
                <property name="orientation">1</property>
                <property name="margin-bottom">24</property>
                <property name="margin-end">24</property>
                <property name="margin-start">24</property>
                <property name="margin-top">24</property>
                <property name="spacing">24</property>
                <child>
                  <object class="GtkLabel">
                    <property name="label">AÃ±adir Articulos</property>
                    <style>
                      <class name="title-1"/>
                    </style>
                  </object>
                </child>
                <child>
                  <object class="GtkScrolledWindow">
                    <property name="hexpand">true</property>
                    <property name="vexpand">true</property>
                    <child>
                      <object class="GtkFlowBox" id="_articulos">
                        <property name="margin-top">12</property>
                        <property name="margin-bottom">12</property>
                        <property name="margin-start">12</property>
                        <property name="margin-end">12</property>
                        <property name="orientation">0</property>
                        <property name="row-spacing">6</property>
                        <property name="column-spacing">6</property>
                        <property name="homogeneous">true</property>
                        <property name="selection-mode">0</property>
                        <child>
                          <object class="GtkFlowBoxChild">
                            <child>
                              <object class="GtkButton" id="_addArticuloButton">
                                <property name="width-request">98</property>
                                <property name="height-request">128</property>
                                <property name="valign">1</property>
                                <style>
                                  <class name="card"/>
                                </style>
                                <child>
                                  <object class="GtkImage">
                                    <property name="icon-name">value-increase-symbolic</property>
                                    <property name="pixel-size">64</property>
                                  </object>
                                </child>
                              </object>
                            </child>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>